{
  "name": "ACDao2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sc": "shadow-cljs",
    "server": "shadow-cljs stop && shadow-cljs start",
    "start": "shadow-cljs watch app",
    "cards": "shadow-cljs watch cards",
    "build": "yarn clean && shadow-cljs release app",
    "test": "shadow-cljs compile test && node out/test.js",
    "test:watch": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "lint": "clj-kondo --lint src",
    "format": "zprint-clj --hang -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "clean": "rimraf public/js"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.19.0",
    "@testing-library/react": "^10.4.3",
    "autoprefixer": "^10.4.0",
    "clj-kondo": "2020.11.7",
    "cssnano": "^5.0.9",
    "highlight.js": "10.1.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "marked": "^1.1.0",
    "postcss-cli": "^9.0.2",
    "rimraf": "~3.0.2",
    "serve-handler": "~6.1.3",
    "shadow-cljs": "~2.10.13",
    "taiko": "~1.0.12",
    "taiko-diagnostics": "~0.4.0",
    "tailwindcss": "^2.2.19",
    "zprint-clj": "~0.8.0"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "^2.3.7",
    "@taquito/beacon-wallet": "^10.2.1",
    "@taquito/taquito": "^10.2.1",
    "create-react-class": "~15.6.3",
    "invariant": "^2.2.4",
    "react": "~16.8.0",
    "react-dom": "~16.8.0",
    "react-refresh": "^0.10.0",
    "redux-observable": "^2.0.0",
    "reselect": "^4.1.2",
    "rxjs": "^6.6.7"
  }
}
